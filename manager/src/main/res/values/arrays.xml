<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <string-array name="night_mode" tools:ignore="InconsistentArrays">
        <item>@string/dark_theme_off</item>
        <item>@string/dark_theme_on</item>
        <item>@string/dark_theme_auto_time</item>
    </string-array>

    <integer-array name="night_mode_value" tools:ignore="InconsistentArrays">
        <item>1</item>
        <item>2</item>
        <item>0</item>
    </integer-array>

    <string-array name="language" translatable="false">
        <item>@string/follow_system</item>
        <item>en</item>
        <item>uk</item>
        <item>zh-Hans</item>
        <item>zh-Hant</item>
    </string-array>

    <string-array name="language_value" translatable="false">
        <item>SYSTEM</item>
        <item>en-US</item>
        <item>uk</item>
        <item>zh-CN</item>
        <item>zh-HK</item>
    </string-array>
</resources>